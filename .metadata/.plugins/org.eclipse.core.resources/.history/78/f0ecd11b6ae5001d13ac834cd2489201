package ch03;

public class WhileEx03 {
	public static void main(String[] args) {
		//구구단
		/*
		 * int dan, num; for( dan = 2; dan<10; dan++) { for(num=1; num<=9; num++) {
		 * System.out.println(dan+"x"+ num +"="+(dan*num)); } }
		 */
		int a=2;
		int b=1;
		while(a<10) {
			while(b<10) {
				System.out.println(a+"x"+ b +"="+(a*b));
				b++;
			}
			System.out.println(a);
			a++;
		}
		
		
	}
}
