package ch03;

public class ForEx08 {// 선생님이랑 하나 차이남
	public static void main(String[] args) {
		//for문속 for문속 for문
		
		int a;
		for(a=1; a<=5; a++) {
			for(int x=1; x<=5; x++) {
				System.out.println(a+"//"+x);
				
			}
			for(int y=1; y<=5; y++) {
				System.out.println(a+" hello");
			}
		}
		
	}
}
