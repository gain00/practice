package ch03;

public class SwitchEx01 {
	public static void main(String[] args) {
		// Switch문 변수의 값을 가지고 평가한다.
		// 결과값을 int와 String 타입으로 사용한다.
		// {}블럭만 영역이다.
		
		int a = 30;
		switch(a) {
		case 50 :
			System.out.println("50");
		case 30 :
			System.out.println("30");
		case 10 : 
			System.out.println("10");
			
		}
		
	}
}
