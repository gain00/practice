package Quiz;

public class ForQ5 {
	public static void main(String[] args) {
		int a,b;
		for(a=1; a<=5; a++) {
			for(b=0; b<=a; b++) {
				System.out.print(" ");
			}
			
			for(b=1; b<=9; b+=2) {
				System.out.print("*");
			}
			
			System.out.println();
		}
	}
}
//a = 1 b= 9  2 7   3 5  4 3 , a=5 b=1       