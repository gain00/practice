package cho2;

public class Ex06 {
	public static void main(String[] args) {
		//문자열 연산 (+)
		//연결한다 의 의미
		String a = "JDK" + 6.0;
		System.out.println(a);
		String b = a + "특징";
		System.out.println(b);
		String c = "JDK"+3+3.0;
		System.out.println(c);
		int d = 3+3;
		System.out.println(d);
		String e = 3+ 3.0+ "JDK";
		System.out.println(e);
	}
}
