package ch01;

public class ForEx {
	public static void main(String[] args) {
		//1부터 100까지의 숫자 중에서 3의 배수와 5의 배수를 출력하는 프로그램을 작성해보세요. 
		//단, 3의 배수와 5의 배수 중에서 공통으로 포함되는 숫자는 한 번만 출력해야 합니다.
		int a;
		int b;
		int c;
		for(a=1; a<=100; a++) {
			if(a%3 == 0) {
				System.out.println("3의배수: "+a);
				
			}else if(a%5==0) {
				System.out.println("5의배수: " +a);
				
			}
				
		}
	
	
	}
}
